{
  "name": "@headlessui/astro",
  "version": "0.1.0",
  "description": "Completely unstyled, fully accessible UI components for Astro",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external astro",
    "dev": "tsup src/index.ts --format esm,cjs --dts --external astro --watch",
    "lint": "eslint . --ext .ts,.astro",
    "clean": "rimraf ./dist"
  },
  "keywords": [
    "astro",
    "headlessui",
    "ui",
    "components",
    "accessible",
    "tailwindcss"
  ],
  "peerDependencies": {
    "astro": "^4.0.0"
  },
  "devDependencies": {
    "astro": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-astro": "^0.27.0",
    "rimraf": "^5.0.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tailwindlabs/headlessui.git",
    "directory": "packages/@headlessui-astro"
  },
  "author": "Tailwind Labs",
  "license": "MIT"
}